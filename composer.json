{
	"name"             : "kevinfrom/ga4mp",
	"version"          : "1.0.1",
	"description"      : "A PHP library for interacting with Google Analytics 4 using the Measurement Protocol API.",
	"type"             : "library",
	"license"          : "MIT",
	"scripts"          : {
		"test": "phpunit"
	},
	"autoload"         : {
		"psr-4": {
			"kevinfrom\\GA4MP\\": "src/"
		}
	},
	"autoload-dev"     : {
		"psr-4": {
			"kevinfrom\\GA4MP\\Tests\\": "tests/"
		}
	},
	"authors"          : [
		{
			"name" : "Kevin From",
			"email": "kevin@kevinfrom.dk"
		}
	],
	"minimum-stability": "stable",
	"require"          : {
		"php"              : ">=7.4",
		"guzzlehttp/guzzle": "^7.9"
	},
	"require-dev"      : {
		"phpunit/phpunit": "^11.3",
		"vlucas/phpdotenv": "^5.6"
	}
}
